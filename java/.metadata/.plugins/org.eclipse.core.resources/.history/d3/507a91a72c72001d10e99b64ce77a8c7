package day17;

import java.util.ArrayList;

public class Ex01 {
	public static void main(String[] args) {
		
		ArrayList <String> al1 = new ArrayList<String>();
		for(int i = 0; i < 10; i++) {
			al1.add("이름" + (i + 1));
		}
		
		System.out.println(al1);
		
		for(int i = 0; i < al1.size(); i++) {
			String name = al1.get(i);
			System.out.println(name);
		}
		
	}
}
