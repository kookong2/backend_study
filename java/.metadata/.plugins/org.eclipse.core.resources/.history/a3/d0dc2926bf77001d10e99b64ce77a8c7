package day22.sub1;

public class Ex03 {
	public static void main(String[] args) {
		
		
	}
}

class ThreadEx3_1 implements Runnable{
	public void run() {
		for(int i = 0; i < 300; i++) {
			System.out.print(new String("-"));
			
			for(long j = 0; j < 200000000000000L; j++);
		}
	}
}

class ThreadEx3_2 implements Runnable{
	public void run() {
		for(int i = 0; i < 300; i++) {
			System.out.print(new String("|"));
			
			for(long j = 0; j < 200000000000000L; j++);
		}
	}
}
