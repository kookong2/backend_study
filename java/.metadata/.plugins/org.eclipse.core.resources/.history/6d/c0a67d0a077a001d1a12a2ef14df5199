package day23;

public class Ex01 {
	public static void main(String[] args) {
		
		Thread thread = new Thread(new ThreadEx01());
		
		
	}
}

class ThreadEx01 implements Runnable{

	@Override
	public void run() {

		for(int i = 1; i <= 10; i++) {
			
			System.out.println(i + "-" + Thread.currentThread().getName());
		}
		
	}
	
}