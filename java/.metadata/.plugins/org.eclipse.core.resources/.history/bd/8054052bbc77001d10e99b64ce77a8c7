package day22.sub1;

public class Ex02 {
	public static void main(String[] args) {
		
		
	}
	
	class ThreadEx3_1 extends Thread{
		public void run() {
			try {
				throwException();				
			}catch (RuntimeException e) {
				e.printStackTrace();
			}
		}
		
		private void throwException() {
			throw new RuntimeException();
		}
	}
}
