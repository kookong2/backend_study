package day17;

import java.util.ArrayList;
import java.util.ListIterator;

public class Ex04 {
	public static void main(String[] args) {
		
		ArrayList<String> names = new ArrayList<String>();
		for(int i = 0; i < 10; i++) {
			names.add("이름" + (i + 1));
		}
		
		ListIterator<String> ir = names.listIterator();
		
		while(ir.hasNext()) {
			String name = ir.next();
			System.out.println(name);
		}
	}
}
