package day20.sub1;

import java.util.List;

import day20.Box;
import day20.Fruit;

public class Jucier {
	public static void make(Box<? extends Fruit>box, Box<? extends Fruit> box2) {	// 컴파일 되면 FruitBox<Fruit>

		System.out.println(box.get().toStrng() + "주스!");
		System.out.println(box2.get().toStrng() + "주스!");

	}
	
	public static <T extends Fruit,E entends Fruit> void make2(Box<T> box, Box<E> box2) {
		System.out.println(box.get().toStrng() + "주스!");
		System.out.println(box2.get().toStrng() + "주스!");
	}
}
