package day23;

import org.w3c.dom.traversal.NodeIterator;

public class Ex03 {
	public static void main(String[] args) {
		
		
	}
}

class ThreadEx03 implements Runnable{

	@Override
	public void run() {
		
		while(true) {
			
			try {
				Thread.sleep(3000);
			} catch (InterruptedException e) {}
		}
	}
	
	private void saved() {
		System.out.println("저장되었습니다.");
	}
	
}
