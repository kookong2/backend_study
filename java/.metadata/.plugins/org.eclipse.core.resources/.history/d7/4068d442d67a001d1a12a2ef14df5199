package day24;


public class Ex01 {
	public static void main(String[] args) {
		
		/*
		Calculator calculator = (x,y) -> x + y;
		int result = calculator.add(10, 20);
		System.out.println(result);
		*/
		
		Calculator calculator = new Calculator() {
			
			@Override
			public int add(int num1, int num2) {
				
				return num1 + num2;
			}
		};
	}
}
