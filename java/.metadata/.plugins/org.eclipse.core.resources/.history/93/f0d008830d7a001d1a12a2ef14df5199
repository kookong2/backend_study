package day23;

public class Ex04 {
	public static void main(String[] args) {
		
		
	}
}

class ThreadEx04 implements Runnable{

	@Override
	public void run() {

		try {
			Thread.sleep(3000);
		} catch (InterruptedException e) {}
	}
	
	private void saved() {
		System.out.println("저장되었습니다.");
	}
	
}