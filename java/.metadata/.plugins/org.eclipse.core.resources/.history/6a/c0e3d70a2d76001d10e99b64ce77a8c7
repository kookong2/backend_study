package day20.sub1;

import java.util.List;

import day20.Box;
import day20.Fruit;

public class Jucier<T> {
	
	private T data;
	
	public static void make(day20.sub1.Box<? extends Fruit>box, day20.sub1.Box<? extends Fruit> box2) {	// 컴파일 되면 FruitBox<Fruit>

		System.out.println(box.get().toString() + "주스!");
		System.out.println(box2.get().toString() + "주스!");

	}
	
	public static <T extends Fruit,E extends Fruit> void make2(day20.sub1.Box<T> box, day20.sub1.Box<E> box2) {
		System.out.println(box.get().toString() + "주스!");
		System.out.println(box2.get().toString() + "주스!");
	}
}
