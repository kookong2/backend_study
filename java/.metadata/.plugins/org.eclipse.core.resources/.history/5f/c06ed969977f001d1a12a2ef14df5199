package day28.sub1;

import java.net.*;
import java.util.Arrays;

public class Ex01 {
	public static void main(String[] args) throws Exception {
		
		InetAddress ia = InetAddress.getByName("wwww.naver.com");
		
		byte[] addr = ia.getAddress();	// 0~255
		System.out.println(Arrays.toString(addr));
	}
}
