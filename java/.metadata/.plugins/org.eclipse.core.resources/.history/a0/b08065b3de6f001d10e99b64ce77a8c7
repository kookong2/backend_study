package day14.sub2;

import java.util.Scanner;

public class Ex03 {
	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		while(true) {
			System.out.print("숫자 2개를 입력 하세요. ㅇㅖ)20 30 ");
			String nums = scanner.nextLine();
			System.out.println("입력한 숫자는 " + nums);
		}
	}
}
