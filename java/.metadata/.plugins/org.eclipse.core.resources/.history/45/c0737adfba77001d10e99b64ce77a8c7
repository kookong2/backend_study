package day22.sub1;

import java.security.PublicKey;

public class Ex01 {
	public static void main(String[] args) {
		
	}
	
	class ThreadEx02_1 extends Thread{
		
		public void run() {
			for(int i = 1; i <= 10; i++) {
				System.out.println(i + "-" + getName());
			}
		}
	}
	
	class ThreadEx2_2 implements Runnable{
		public void run() {
			for(int i = 1; i < 10; i++) {
				Thread th = Thread.currentThread();
				System.out.println(i + "-" + th.getName());
			}
		}
	}
}
